package com.ruyicai.wap.rss.bean;

import java.util.Date;
import java.util.Set;

/**
 * @author zhaokailong
 * @category ruyicai
 */
public class Channel {
	private String title="RSS--如意彩手机新闻资讯"; //频道标题
	private String link;//频道连接
	private String language="zh_cn";
	private String copyright="Copyright© 2009-2014如意彩彩票网 All rights reserved";
	private String generator="如意彩--RUYICAI.COM ,最专业的手机购彩平台";
	private String description="如意彩-彩民趣事";//对列表项目的简短说明, 
	private String ttl; //频道的过期时间段
	private Date pubDate;//发布时间.
	private Set<Items> items;
	private Set<Image> image;
	public Date getPubDate() {
		return pubDate;
	}

	public void setPubDate(Date pubDate) {
		this.pubDate = pubDate;
	}
	public String getTtl() {
		return ttl;
	}

	public void setTtl(String ttl) {
		this.ttl = ttl;
	}
	

	@Override
	public String toString() {
		return "Channel [title=" + title + ", link=" + link + ", language="
				+ language + ", copyright=" + copyright + ", generator="
				+ generator + ", description=" + description + ", ttl=" + ttl
				+ ", pubDate=" + pubDate + ", items=" + items + ", image="
				+ image + "]";
	}

	public Set<Image> getImage() {
		return image;
	}

	public void setImage(Set<Image> image) {
		this.image = image;
	}

	
	public Channel() {
		super();
	}

	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getLink() {
		return link;
	}
	public void setLink(String link) {
		this.link = link;
	}
	public String getLanguage() {
		return language;
	}
	public void setLanguage(String language) {
		this.language = language;
	}
	public String getCopyright() {
		return copyright;
	}
	public void setCopyright(String copyright) {
		this.copyright = copyright;
	}
	public String getGenerator() {
		return generator;
	}
	public void setGenerator(String generator) {
		this.generator = generator;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public Set<Items> getItems() {
		return items;
	}
	public void setItems(Set<Items> items) {
		this.items = items;
	}
	
	
}
