<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN" "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
<!--
    Configuration file for UrlRewriteFilter http://tuckey.org/urlrewrite/
-->
<urlrewrite>

      <rule>
          <name>html list</name>
           <from>/([0-9A-Za-z]+)/([0-9A-Za-z]+).html</from>
           <to>/userWap.do?method=toRecommendRegister&amp;redMobile=$1&amp;rededMobile=$2</to>
       </rule>

</urlrewrite>

