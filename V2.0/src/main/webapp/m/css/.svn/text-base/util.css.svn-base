@charset "utf-8";
/* 公共样式 */
html,body,p,form,div,table,textarea,input,dt,dd{font-size: 16px; color:#333;vertical-align:left; }
html{ background:#f3f3f3; width:100%;}
body{color:#FFFFFF;-webkit-text-size-adjust: none; position:relative; font-family:"微软雅黑",Arial, Helvetica, sans-serif;line-height:180%;}
body,div,p,a,table,td,textarea,form,input,img,ul,li,dt,dd,h1,h2,h3{margin:0;padding:0;}
li{ list-style:none; text-indent:0;}
img,a img{ border:0;}
form{border:0;margin:0;}
img{border:0;vertical-align:middle}
.clear10 {clear:both;height:5px;display:block;}
a{color:#666;text-decoration:none;}
.main{ width:100%; }
.min-head{background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#fafafa), to(#c3c3c3));background:-moz-linear-gradient(0% 0% 270deg, #fafafa, #c3c3c3 100%); height:50px; padding-left:10px; width:100%;}
.OutlookBr{ background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#dfdfdf), to(#f0f0f0));background:-moz-linear-gradient(0% 0% 270deg, #dfdfdf, #f0f0f0 100%);  height:40px;  padding-left:10px; overflow:hidden;}
.OutlookBr span{ float:left; margin-right:5px; line-height:40px; color:#666;font-size: 18px; font-weight:bold;}
.nav{ border:1px solid #e3e3e3;-moz-border-radius:6px; height:35px; margin-left:10px;margin-right:10px;}
.nav dl{ margin:0; padding:0;}
.nav dd { display:block;background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#f9f9f9), to(#e9e9e9));background:-moz-linear-gradient(0% 0% 269deg, #f9f9f9, #e9e9e9 100%); border-left:#d3d3d3 1px solid; width:24.7%; float:left; line-height:35px;font-size:13px; text-align:center; font-weight:bold;}
.nav dt { display:block;background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#f9f9f9), to(#e9e9e9));background:-moz-linear-gradient(0% 0% 269deg, #f9f9f9, #e9e9e9 100%); width:24.7%;  float:left; line-height:35px;font-size:13px; text-align:center; font-weight:bold;}
.nav .nav_over{display:block;background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#e9e9e9), to(#f0f0f0));background:-moz-linear-gradient(0% 0% 269deg, #e9e9e9, #f0f0f0 100%); width:24.7%;   float:left; line-height:35px;font-size:13px; text-align:center; font-weight:bold;}
.inputbig{height:20px;width:150px;border:1px solid #BFC1C6;margin-left:5px; margin-right:5px;}
.inputsmall{height:20px;width:75px;border:1px solid #BFC1C6; margin-left:5px; margin-right:5px;}
.selectbig{width:150px;height:20px;border: 1px solid #BFC1C6;-webkit-border-radius: 3px;color:#333;font-weight: normal;padding: 2px; background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#f8fafb), to(#e4eaef));background:-moz-linear-gradient(0% 0% 269deg, #f8fafb, #e4eaef 100%);font-size:12px;line-height:35px;margin-left:5px; margin-right:5px;}
.selectsmall{width:75px;height:20px;border: 1px solid #BFC1C6;-webkit-border-radius: 3px;color:#333;font-weight: normal;padding: 2px; background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#f8fafb), to(#e4eaef));background:-moz-linear-gradient(0% 0% 269deg, #f8fafb, #e4eaef 100%);font-size:14px;line-height:20px;margin-left:5px; margin-right:5px;}
.redtext{ color:#ca003d;}
.bluetext{ color:#399eb8;}
.f07264{ color:#f07264;}
/* 首页样式 */
.lottory{ margin-left:10px; padding:8px; margin-right:10px; height:auto; background:#fff; border:1px solid #d2d2d2;-moz-border-radius:10px 10px 0 0;}
.lottory ul{ margin:0; padding:0; overflow:hidden;}
.lottory li{ width:63px; margin:2px 15px; float:left;}
.lottory li a{ text-align:center; line-height:25px; display:block; font-weight:bold; color:#333; font-size:12px;}
.button{margin-right:10px; height:32px; padding:6px 40px; margin-left:10px;background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#fbfbfb), to(#eeeeee));background:-moz-linear-gradient(0% 0% 269deg, #fbfbfb, #eeeeee 100%);border:1px solid #d2d2d2; border-top:none;-moz-border-radius:0 0 10px 10px; overflow:hidden;}
.button a{ height:25px; width:75px; display:block;background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#fff), to(#e3e3e3));background:-moz-linear-gradient(0% 0% 269deg, #fff, #e3e3e3 100%);border:1px solid #d2d2d2;-moz-border-radius:5px;font-size: 12px; font-weight:bold; text-align:center; line-height:25px; float:left;}
.button a:hover{background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#e3e3e3), to(#fff));background:-moz-linear-gradient(0% 0% 269deg, #e3e3e3, #fff 100%);}
.input1{ margin-right:62px;}


/* 提现样式 */
.lottoryTx{ margin-left:10px; padding:10px 8px; margin-right:10px; height:auto; background:#fff; border:1px solid #d2d2d2; border-bottom:none;-moz-border-radius:10px 10px 0px 0px;}
.lottoryTx table th{ font-size:12px; color:000; text-align:right; line-height:30px;}
.prompt{ color:#990000; font-size:11px; line-height:20px; padding-left:22px;}

.buttonTx{margin-right:10px; height:22px; padding:7px 40px; margin-left:10px;background:f5f5f5;border:1px solid #d2d2d2; -moz-border-radius: 0px 0px 10px 10px; overflow:hidden;}
.buttonTx a{ height:20px; width:75px; display:block;background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#fff), to(#e3e3e3));background:-moz-linear-gradient(0% 0% 269deg, #fff, #e3e3e3 100%);border:1px solid #d2d2d2;-moz-border-radius:7px;font-size: 12px; font-weight:bold; text-align:center; line-height:20px; float:left; color:#000;}
.buttonTx a:hover{background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#e3e3e3), to(#fff));background:-moz-linear-gradient(0% 0% 269deg, #e3e3e3, #fff 100%);}


/* 开奖样式 */
.lottoryKj{ margin-left:10px; padding:8px;margin-right:10px; height:auto; background:#fff; border:1px solid #d2d2d2; -moz-border-radius:7px; overflow:hidden;}
.lotteryssq{ background:url(../images/lotterylogossq.jpg) no-repeat; width:45px; height:46px; display:block;}
.lottery3D{ background:url(../images/lotterylogo3D.jpg) no-repeat; width:45px; height:46px; display:block;}
.lotteryqlc{ background:url(../images/lotterylogoqlc.jpg) no-repeat; width:45px; height:46px; display:block;}
.lotteryzi{ width:44px; height:18px; text-align:center; margin-top:4px; border:1px solid #d6d6d6; -moz-border-radius:7px;display:block;background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#fafafa), to(#dedede));background:-moz-linear-gradient(0% 0% 269deg, #fafafa, #dedede 100%);}
.lotteryzi a{ font-size:12px; line-height:18px; color:#000; font-weight:bold;}
.lottoryKj_left{ width:45px; float:left; height:70px;padding-right:10px;}
.lottoryKj_top{  height:35px; font-size:12px; line-height:35px; padding-left:10px; color:#000;}
.lottoryKj_below{ float:left;height:35px;}
.lottoryKj_below ul{ margin:0; padding:0; display:block; }
.lottoryKj_below ul li{ width:30px; height:33px;  margin-right:3px; float:left; line-height:30px; text-align:center; font-size:14px;color:#000; font-weight:bold;}
.redball{background:url(../images/redball.gif) no-repeat;}
.blueball{background:url(../images/blueball.gif) no-repeat;}


/* 更多样式 */
.lottorymore{margin-left:10px; margin-right:10px; background:#fff; border:1px solid #d2d2d2; -moz-border-radius:7px; overflow:hidden;-moz-box-shadow: 2px 4px 5px #dbdbdb;}
.lottorymore table td{ border-bottom:1px solid #d2d2d2; line-height:28px; font-size:12px; font-weight:bold;}
.lottorymore table th{text-align:left;line-height:28px; font-size:12px;}
.icons{  background:url(../images/icons.gif) no-repeat; width:13px; height:12px; display:block; margin-left:8px;}
.ic1{ background-position:0 0;}
.ic2{ background-position:0 -25px;}
.ic3{ background-position:0 -50px;}
.ic4{ background-position:0 -75px;}
.ic5{ background-position:0 -100px;}
.ic6{ background-position:0 -125px;}
.ic7{ background-position:0 -150px;}

.account{ height:32px; line-height:32px; font-size:14px; margin-left:10px; padding-left:10px; font-weight:bold;}
.lottoryaccount{margin-left:10px; margin-right:10px;  background:#fff; border:1px solid #d2d2d2; -moz-border-radius:7px; padding:10px 15px;}
.lottoryaccount p{font-size:12px; line-height:200%; color:#000;}
.lottoryaccount table td{font-size:12px; line-height:200%; color:#000;}
.lottorydetail{margin-left:10px;  margin-right:10px; background:#fff; border:1px solid #d2d2d2; border-bottom:none; -moz-border-radius:7px 7px 0 0; padding:10px 15px;}
.lottorydetail p{font-size:12px; line-height:200%; color:#000;}
.lottorydetailnext{margin-left:10px; margin-right:10px;padding-left:10px; background:#fff; border:1px solid #d2d2d2; -moz-border-radius: 0 0 7px 7px; padding:10px 15px;}
.lottorydetailnext p{font-size:12px; line-height:200%; color:#000;}


/* 投注样式 */
.touzhuTop{ margin-right:10px; height:33px; margin-left:10px; overflow:hidden;}
.logo{ width:30px; height:31px; display:block; float:left;}
.logossq{ background:url(../images/logossq.jpg) no-repeat; }
.logo3D{ background:url(../images/logo3D.jpg) no-repeat;}
.logoqlc{ background:url(../images/logoqlc.jpg) no-repeat;}
.logozi{ font-size:14px; color:#333; font-weight:bold; line-height:33px; margin-left:3px; float:left;}
.return{ float:right; margin-right:10px; margin-top:5px;}
.return a{ text-align:center; font-size:12px; font-weight:bold; line-height:20px; display:block; width:60px; height:20px;background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#fff), to(#e2e2e2));background:-moz-linear-gradient(0% 0% 269deg, #fff, #e2e2e2 100%);border:1px solid #d2d2d2;-moz-border-radius:5px;}
.return a:hover{background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#e2e2e2), to(#fff));background:-moz-linear-gradient(0% 0% 269deg, #e2e2e2, #fff 100%);}

.touzhuCon{  margin-right:10px;margin-left:10px; background:#f5f5f5; border:1px solid #d2d2d2;-moz-border-radius:10px 10px 0 0 ;}
.touzhuCon p{ color:#ca003b; font-size:14px; line-height:180%; padding-left:11px;}
.touzhunav{ padding-top:5px; padding-left:11px;}
.touzhunav ul{ margin:0; padding:0; display:block; overflow:hidden;}
.touzhunav ul li{ background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#fff), to(#e4e4e4));background:-moz-linear-gradient(0% 0% 269deg, #fff, #e4e4e4 100%);border:1px solid #c6c6c6;-moz-border-radius:5px 5px 0 0; width:18%; height:20px; font-size:12px; float:left; text-align:center; line-height:20px;}
.touzhunav .navover{background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#f06f62), to(#cd073d));background:-moz-linear-gradient(0% 0% 269deg, #f06f62, #cd073d 100%); color:#fff;}


.touzhutext{  margin-right:10px; padding:0 25px 5px;margin-left:10px; height:auto; background:#fff; border:1px solid #d2d2d2; border-top:none;-moz-border-radius:0 0 10px 10px ;}
.touzhutext p{ font-size:12px; line-height:200%; color:#333;}
.tijiao a{text-align:center; font-size:12px; font-weight:bold; line-height:20px; height:20px;background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#fff), to(#e2e2e2));background:-moz-linear-gradient(0% 0% 269deg, #fff, #e2e2e2 100%);border:1px solid #d2d2d2;-moz-border-radius:3px; padding:3px 8px;}
.tijiao a:hover{background:-webkit-gradient(linear, 0% 100%, 0% 0%, from(#e2e2e2), to(#fff));background:-moz-linear-gradient(0% 0% 269deg, #e2e2e2, #fff 100%);}
